class Solution {
    public int singleNumber(int[] nums) {
        Arrays.sort(nums);
        int temp = 0;
        for(int i = 0; i< nums.length -1;)
        {
            if(nums[i] != nums[i+1] )
            {
                return nums[i];
            }
           i = i+2; 
        }
        
        return nums[nums.length -1];
    }
}